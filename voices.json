{
  "defaultPaths": [
    {
      "name": "ONNX-TTS (Documentos)",
      "path": "Documents/ONNX-TTS",
      "description": "Carpeta predeterminada para modelos ONNX-TTS en Documentos del usuario",
      "enabled": true
    }
  ],
  "customPaths": [],
  "settings": {
    "autoScan": true,
    "scanInterval": 30000,
    "supportedFormats": [".onnx", ".onnx.json"],
    "maxModelSize": "2GB"
  },
  "audioSettings": {
    "defaultSpeaker": 0,
    "defaultNoiseScale": 0.667,
    "defaultLengthScale": 1.0,
    "defaultNoiseW": 0.8,
    "outputFormat": "mp3",
    "sampleRate": 22050,
    "channels": 1
  },
  "ui": {
    "theme": "dark",
    "language": "es",
    "autoSaveText": true,
    "showModelImages": true,
    "compactMode": false
  },
  "advanced": {
    "piperPath": "./piper/piper.exe",
    "ffmpegPath": "./ffmpeg.exe",
    "tempDirectory": "temp",
    "maxConcurrentJobs": 1,
    "enableLogging": true,
    "logLevel": "info"
  }
}
